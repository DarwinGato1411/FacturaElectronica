                                            tooltiptext="Buscar"
                                        />

                                     
                                    </div>
                                    <div class="wrapper_input">
                                        <span>Cliente:</span>
                                        <textbox 
                                            value="@bind(vm.clienteBuscado.cliNombre)" 
                                            class="input" 
                                            readonly="true" 
                                        />
                                    </div>
                                    <div class="wrapper_input">
                                        <span>Emisión:</span>
                                    
                                        <datebox 

