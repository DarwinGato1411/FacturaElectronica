                                            readonly="true"
                                            onChange="self.setValue(self.getValue().toUpperCase().trim());" 
                                            class="input" 
                                        />
                                    </div>
                                    <div class="wrapper_input">
                                        <span>Base 12%:</span>
                                        <decimalbox  
                                            format="###.##"   
                                            value="@bind(vm.subTotalCotizacion)" 
                                            readonly="true"
                                            onChange="self.setValue(self.getValue().toUpperCase().trim());" 
                                            class="input" 
                                        />
                                    </div>
                               
                                    <div class="wrapper_input">
                                        <span>Base 13%:</span>
                                        <decimalbox  
                                            format="###.##"   
