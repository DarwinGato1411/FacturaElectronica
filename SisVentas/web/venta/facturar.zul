                                    <div class="wrapper_input">
                                        <span>IVA 5%:</span>
                                        <decimalbox  
                                            format="###.##"  
                                            value="@bind(vm.ivaCotizacion5)"
                                            readonly="true" 
                                            class="input" 
                                        />
                                    </div>
                                    <div class="wrapper_input">
                                        <span>IVA 12%:</span>
                                        <decimalbox  
                                            format="###.##"  
                                            value="@bind(vm.ivaCotizacion)"
                                            readonly="true" 
                                            class="input" 
                                        />
                                    </div>
                                    <div class="wrapper_input">
                                        <span>IVA 13%:</span>
                                        <decimalbox  
                                            format="###.##"  
                                            value="@bind(vm.ivaCotizacion13)"
                                            readonly="true" 
                                            class="input" 
                                        />
                                    </div>
                                    <div class="wrapper_input">
                                        <span>IVA 14%:</span>
                                        <decimalbox  
                                            format="###.##"  
                                            value="@bind(vm.ivaCotizacion14)"
                                            readonly="true" 
                                            class="input" 
                                        />
                                    </div>
                                    <div class="wrapper_input">
                                        <span>IVA 15%:</span>
                                        <decimalbox  
                                            format="###.##"  
                                            value="@bind(vm.ivaCotizacion15)"
                                            readonly="true" 
                                            class="input" 
                                        />
                                    </div>
                                

                                </div>
                                <div class="wrapper_data">
                                    <div class="wrapper_input">
                                        <span>Base 0%:</span>
                                        <decimalbox  
                                            format="###.##"   
                                            value="@bind(vm.subTotalBaseCero)" 
                                            readonly="true"
                                            onChange="self.setValue(self.getValue().toUpperCase().trim());" 
                                            class="input" 
                                        />
                                    </div>
                                    
                                    <div class="wrapper_input" style="flex:auto">
                                        <!--<span   >-->
                                            <label style="color:#0275d8;font-size:20px !important; font-weight: bold !important;" value="Total :"> </label> 
                                        <!--</span>-->
                                        <!--<div class="wrapper_input_text" style="width:100%">-->
                                            <!--<div class="fa-solid fa-dollar-sign icon"></div>-->
                                            <decimalbox  
                                            format="###.##"   
                                            value="@bind(vm.valorTotalCotizacion)"                                                 
                                            readonly="true"
                                            style="color:#0275d8;font-size:25px !important; font-weight: bold !important;
                                                    padding-top: 0px !important;
                                                    padding-bottom: 0px !important;
                                                    margin-top: 0px;
                                                    background-color: #e1e1e1;
                                                    margin-bottom: 0px;
                                                     width: 150px;"
                                        />
                                        <!--</div>-->
                                    </div>
                                    <div class="wrapper_input" style="">
                                        <!--<span>Cobrar:</span>-->
                                        <label style="color:#02d895;font-size:20px !important; font-weight: bold !important;" value="Cobrar:"> </label> 
                                        <textbox style="color:#02d895;font-size:25px !important; font-weight: bold !important;
                                                    padding-top: 0px !important;
                                                    padding-bottom: 0px !important;
                                                    margin-top: 0px;
                                                    margin-bottom: 0px;
                                                     width: 150px;"
                                                 class="input" 
                                                 constraint="/^[0-9]{1,9}(?:[.][0-9]{1,2})?$/:Ingrese solo numeros" 
                                                 onOK="@command('calcularCambio')"
                                                 value="@bind(vm.cobro)"
                                        />
                                    </div>
                                
                                    <div class="wrapper_input">
                                        
                                        <label style="color:#d9534f;font-size:20px !important; font-weight: bold !important;" value="Cambio:"> </label> 
                                        <decimalbox  style="color:#d9534f;font-size:25px !important; font-weight: bold !important;
                                                    padding-top: 0px !important;
                                                    padding-bottom: 0px !important;
                                                    margin-top: 0px;
                                                    margin-bottom: 0px;
                                                     background-color: #e1e1e1;
                                                    width: 150px;"
                                                     format="###.##"   
                                                     readonly="true" 
                                                     class="input"
                                                     value="@bind(vm.cambio)" 
                                        />
                                    </div>
                                    <div class="wrapper_input">
                                        <button src="/iconos/fact.png"   
                                                autodisable="self"
                                                label="NOTA ENTREGA"
                                                width="100%"                                                                                                   
                                                class="btn btn btn-secondary btn-sm "
                                                onClick="@command('verNotasEntrega')"
                                                tooltiptext="Cargar notas de entrega"/>
<!--                                    </div>
                                    <div class="wrapper_input">-->
                                        <button 
                                            autodisable="self"   
                                            class="fa-solid fa-floppy-disk btn_fact btn_primary"                  
                                            onClick="@command('Guardar',valor='SG')"
                                            tooltiptext="Guardar"
                                            label="FACTURAR"
                                            width="100%"
                                        />

                                    </div>
                                    

<!--                                    <div class="wrapper_input">
                                        <span style="text_total">Total factura:</span>

                                        
                                    </div>-->
                                 
                             
                                </div>
                            
                            </div>
                        </div>

                        <div class="factura_body factura_body_content">
                    

                            <div class="factura_body_item facturacion" >
                                <listbox model="@load(vm.listaKardexProducto)" 
                                         nonselectableTags=""    mold="paging" pageSize="10"  >
                                    <custom-attributes org.zkoss.zul.listbox.rod="true"/>

            
                                    <listhead class="encabezado1"> 
                                        <listheader width="100%" align="left" class="tableHeader" >
                                            <textbox value="@bind(vm.buscarCodigoProd)"  
                                    
