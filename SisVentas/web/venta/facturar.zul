          viewModel="@id('vm') @init('com.ec.controlador.Facturar')" width="100%" class="containerFactura">
    
        <timer delay="30000" repeats="true" onTimer="@command('refrescarPagina')" />
        <tabbox  width="100%"  tabscroll="true" >
            <tabs >
                <tab label="Emitir factura" sclass="menutabs"/>
            </tabs>

            <tabpanels >
                
                <tabpanel  height="100%" width="98%" class="factura asdasdsad" >
                    
                    <div class="container-fluid " >


                        
                        <div class="factura_header">
                            <div class="fact_row">
                                <div class="wrapper_data wrapper_data1">
                                    <div class="wrapper_input">
                                        <span>Cédula:</span>
                                        <textbox 
                                            class="input"
                                            ctrlKeys="^A" 
                                            onCtrlKey="@command('buscarClienteEnLista')" 
                                            placeholder="Cedula"
                                            value="@bind(vm.clienteBuscado.cliCedula)" 
                                            onDoubleClick="@command('buscarClienteEnLista')"
                                            onOK="@command('buscarClienteDni',valor=self.getValue())"                                                  
