		<queryString>
			<![CDATA[select cc.cab_num_factura,cc.cab_fecha_emision,rc.rco_fecha,
dr.drc_base_imponible,dr.drc_descripcion,dr.drc_porcentaje,
dr.drc_valor_retenido,cc.cab_fecha,
cc.cab_establecimiento,cc.cab_punto_emi
from retencion_compra rc,
cabecera_compra cc,
detalle_retencion_compra  dr,
tipoambiente ta,
proveedores pr
where rc.id_cabecera=cc.id_cabecera
and rc.rco_codigo=dr.rco_codigo
and ta.cod_tipoambiente=rc.cod_tipoambiente
and pr.id_proveedor=cc.id_proveedor
